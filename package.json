{
	"name": "hapify-cli-console",
	"version": "0.3.0",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --prod --env=prod",
		"build-for-cli": "ng build --prod --env=prod --output-path=../../html",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"sync-i18n": "node ./src/i18n/index.js && ./node_modules/.bin/prettier --write \"src/assets/i18n/*.json\"",
		"link-i18n": "node ./src/i18n/link.js",
		"extract-i18n": "./node_modules/.bin/ngx-translate-extract -m _ --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json && ./node_modules/.bin/prettier --write \"src/assets/i18n/*.json\"",
		"prettier": "./node_modules/.bin/prettier --write \"src/*/**/*{.ts,.json,.css,.scss,.html}\""
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^7.1.2",
		"@angular/cdk": "^7.0.3",
		"@angular/common": "^7.1.2",
		"@angular/compiler": "^7.1.2",
		"@angular/core": "^7.1.2",
		"@angular/elements": "^7.1.2",
		"@angular/forms": "^7.1.2",
		"@angular/http": "^7.1.2",
		"@angular/material": "^7.0.3",
		"@angular/platform-browser": "^7.1.2",
		"@angular/platform-browser-dynamic": "^7.1.2",
		"@angular/pwa": "^0.10.4",
		"@angular/router": "^7.1.2",
		"@angular/service-worker": "^7.1.2",
		"@ng-bootstrap/ng-bootstrap": "^4.0.0",
		"@ng-bootstrap/schematics": "^2.0.0-alpha.1",
		"@ngx-meta/core": "^7.0.0",
		"@ngx-translate/core": "^11.0.0",
		"@ngx-translate/http-loader": "^4.0.0",
		"@schematics/angular": "^7.0.4",
		"@tinymce/tinymce-angular": "^2.2.0",
		"angular2-hotkeys": "^2.0.4",
		"bootstrap": "^4.1.3",
		"core-js": "^2.5.4",
		"document-register-element": "^1.7.2",
		"error-stack-parser": "^2.0.2",
		"fs": "0.0.1-security",
		"hammerjs": "^2.0.8",
		"marked": "^0.6.0",
		"md5": "^2.2.1",
		"ng2-ace-editor": "^0.3.8",
		"ng4-click-outside": "^1.0.1",
		"ngx-bootstrap": "^2.0.0-beta.8",
		"ngx-perfect-scrollbar": "^7.0.0",
		"nl2br-pipe": "^1.1.0",
		"open-iconic": "^1.1.1",
		"rxjs": "^6.3.3",
		"rxjs-compat": "^6.3.3",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.12.1",
		"@angular/cli": "~7.0.4",
		"@angular/compiler-cli": "^7.1.2",
		"@angular/language-service": "^7.1.2",
		"@types/error-stack-parser": "^2.0.0",
		"@types/jasmine": "~2.8.6",
		"@types/jasminewd2": "~2.0.3",
		"@types/md5": "^2.1.33",
		"@types/node": "~10.12.3",
		"codelyzer": "~4.5.0",
		"depcheck": "^0.6.11",
		"googleapis": "27.0.0",
		"husky": "^1.1.3",
		"jasmine-core": "~3.3.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^3.1.1",
		"karma-chrome-launcher": "~2.2.0",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "~2.0.4",
		"karma-jasmine": "~1.1.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"ngx-translate-extract": "1.0.0",
		"prettier": "^1.15.1",
		"pretty-quick": "^1.8.0",
		"protractor": "^5.4.1",
		"ts-node": "~7.0.1",
		"tslint": "~5.11.0",
		"typescript": "~3.1.6"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
