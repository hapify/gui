{
		"name": "hapify-gui",
		"version": "0.4.2",
		"license": "MIT",
		"scripts": {
				"ng": "ng",
				"start": "ng serve",
				"build": "ng build --prod --configuration=production",
				"test": "ng test",
				"lint": "prettier --write '**/*.ts'",
				"e2e": "ng e2e",
				"update": "npm-check -u",
				"sync-i18n": "node ./src/i18n/index.js && ./node_modules/.bin/prettier --write \"src/assets/i18n/*.json\"",
				"link-i18n": "node ./src/i18n/link.js",
				"extract-i18n": "./node_modules/.bin/ngx-translate-extract -m _ --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json && ./node_modules/.bin/prettier --write \"src/assets/i18n/*.json\""
		},
		"dependencies": {
				"@commitlint/cli": "^8.3.5",
				"@commitlint/config-conventional": "^8.3.4",
				"@angular/animations": "~10.0.6",
				"@angular/common": "~10.0.6",
				"@angular/compiler": "~10.0.6",
				"@angular/core": "~10.0.6",
				"@angular/forms": "~10.0.6",
				"@angular/platform-browser": "~10.0.6",
				"@angular/platform-browser-dynamic": "~10.0.6",
				"@angular/router": "~10.0.6",
				"@ng-bootstrap/ng-bootstrap": "^4.0.0",
				"@ng-bootstrap/schematics": "^2.0.0-alpha.1",
				"@ngx-meta/core": "^7.0.0",
				"@ngx-translate/core": "^11.0.0",
				"@ngx-translate/http-loader": "^4.0.0",
				"angular2-hotkeys": "^2.0.4",
				"bootstrap": "^4.1.3",
				"md5": "^2.2.1",
				"ng2-ace-editor": "^0.3.8",
				"ng4-click-outside": "^1.0.1",
				"ngx-bootstrap": "^2.0.0-beta.8",
				"ngx-perfect-scrollbar": "^7.0.0",
				"nl2br-pipe": "^1.1.0",
				"open-iconic": "^1.1.1",
				"rxjs": "~6.5.5",
				"tslib": "^2.0.0",
				"zone.js": "~0.10.3"
		},
		"devDependencies": {
				"@angular-devkit/build-angular": "~0.1000.5",
				"@angular/cli": "~10.0.5",
				"@angular/compiler-cli": "~10.0.6",
				"@types/node": "^12.11.1",
				"@types/jasmine": "~3.5.0",
				"@types/jasminewd2": "~2.0.3",
				"@types/md5": "^2.1.33",
				"codelyzer": "^6.0.0",
				"googleapis": "27.0.0",
				"husky": "^1.1.3",
				"npm-check": "^5.9.2",
				"jasmine-core": "~3.5.0",
				"jasmine-spec-reporter": "~5.0.0",
				"karma": "~5.0.0",
				"karma-chrome-launcher": "~3.1.0",
				"karma-coverage-istanbul-reporter": "~3.0.2",
				"karma-jasmine": "~3.3.0",
				"karma-jasmine-html-reporter": "^1.5.0",
				"ngx-translate-extract": "1.0.0",
				"prettier": "^1.15.1",
				"pretty-quick": "^1.8.0",
				"protractor": "~7.0.0",
				"ts-node": "~8.3.0",
				"tslint": "~6.1.0",
				"typescript": "~3.9.5"
		},
		"husky": {
				"hooks": {
						"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
						"pre-commit": "pretty-quick --staged"
				}
		}
}
