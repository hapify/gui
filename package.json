{
  "name": "hapify-gui",
  "version": "0.4.2",
  "license": "MIT",
  "scripts": {
    "install": "./post-install.sh",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --configuration=production",
    "test": "ng test",
    "lint": "prettier --write '**/*.{ts,js}'",
    "e2e": "ng e2e",
    "update": "npm-check -u",
    "sync-i18n": "node ./src/i18n/index.js && ./node_modules/.bin/prettier --write \"src/assets/i18n/*.json\"",
    "link-i18n": "node ./src/i18n/link.js",
    "extract-i18n": "./node_modules/.bin/ngx-translate-extract -m _ --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json && ./node_modules/.bin/prettier --write \"src/assets/i18n/*.json\""
  },
  "dependencies": {
    "@angular/animations": "~10.0.6",
    "@angular/cdk": "^10.1.2",
    "@angular/common": "~10.0.6",
    "@angular/compiler": "~10.0.6",
    "@angular/core": "~10.0.6",
    "@angular/forms": "~10.0.6",
    "@angular/material": "^10.1.2",
    "@angular/platform-browser": "~10.0.6",
    "@angular/platform-browser-dynamic": "~10.0.6",
    "@angular/router": "~10.0.6",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ng-bootstrap/schematics": "^2.0.0-alpha.1",
    "@ngx-meta/core": "^9.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular2-hotkeys": "^2.2.0",
    "bootstrap": "^4.5.0",
    "md5": "^2.3.0",
    "ng2-ace-editor": "^0.3.9",
    "ng4-click-outside": "^1.0.1",
    "ngx-bootstrap": "^5.6.1",
    "ngx-perfect-scrollbar": "^10.0.1",
    "nl2br-pipe": "^1.2.3",
    "open-iconic": "^1.1.1",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/md5": "^2.2.0",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "googleapis": "27.0.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-translate-extract": "1.0.0",
    "npm-check": "^5.9.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --pattern '**/*.{ts,js}'"
    }
  }
}
